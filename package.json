{
  "build": {
    "appId": "com.example.vtts",
    "productName": "VTTS",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main.js",
      "package.json",
      "nebulaview/build/**/*",
      "resources/**/*",
      "stellarcore/**/*"
    ],
    "publish": {
      "provider": "github",
      "owner": "CestMerNeil",
      "repo": "VTTS",
      "releaseType": "release"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    }
  }
}